            </tr>
            <tr>
                <th>Edadmetabolica</th>
                <td>{{ entity.edadMetabolica }}</td>
            </tr>
            <tr>
                <th>Dcibmr</th>
                <td>{{ entity.dciBmr }}</td>
            </tr>
            <tr>
                <th>Masaosea</th>
                <td>{{ entity.masaOsea }}</td>
            </tr>
            <tr>
                <th>Grasavisceral</th>
                <td>{{ entity.grasaVisceral }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('admin_composicioncorporal') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_composicioncorporal_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
