{% extends '::base.html.twig' %}

{% block body -%}
    <h1>ComposicionCorporal</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Peso</th>
                <td>{{ entity.peso }}</td>
            </tr>
            <tr>
                <th>Grasacorporal</th>
                <td>{{ entity.grasaCorporal }}</td>
