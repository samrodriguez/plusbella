

{% block contenido -%}
 
<div class="panel panel-default">
    <div class="panel-heading"><p><strong>Nuevo descuento</strong></p></div>
  <div class="panel-body">
  {{ form_start(form) }}
    {{ form_errors(form) }}
        <div class="col-xs-12 col-sm-4">
        {{ form_row(form.nombre) }}
        </div>
        <div class="col-xs-12 col-sm-2">
        {{ form_row(form.porcentaje) }}
        </div>
       {# {{ form_row(form.estado) }} #}
   
             <div class="form-grouphide col-sm-9">
                <a href="{{ path('admin_descuento') }}" class="btn btn-default btn-sm" id="cancelarNuevo">Cancelar</a>
                {{ form_widget(form.submit) }}
              </div>

 </div>
{{ form_widget(form._token) }}
</div>  
{{ form_end(form) }} 

{% endblock %}
