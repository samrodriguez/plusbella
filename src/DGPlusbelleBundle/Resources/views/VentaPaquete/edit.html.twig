{% extends 'DGPlusbelleBundle:Layout:general.html.twig' %}


{% block contenido -%}
    <h3 class="modal-title" id="myModalLabel">Paquete vendido</h3>
    <br>
    {#{{ form(edit_form) }}#}
    {{ form_start(edit_form) }}
    {{ form_errors(edit_form) }}
    <div class="row">
        
	        <div class="col-md-6" >

	        	{{ form_row(edit_form.paciente)}}
		
			</div>

			<div class="col-md-6" >
	        
	        	{{ form_row(edit_form.empleado)}}
		
			</div>

			
			<div class="col-md-6" >
		    	{{ form_row(edit_form.paquete)}}
		    </div>			
			<div class="col-md-6" >
				{{ form_row(edit_form.fechaVenta)}}
			</div>

		

    </div>
        <div class="modal-footer">
        	<a href="{{ path('admin_ventapaquete') }}" class="btn btn-default">
	            Regresar a la lista
	        </a>
	        {{ form_widget(edit_form.submit) }}
	     </div>
	        
        
	

		{{ form_end(edit_form) }}

{% endblock %}
