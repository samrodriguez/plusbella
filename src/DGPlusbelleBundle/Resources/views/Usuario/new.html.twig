
{% block contenido -%}
    
    <div class="panel panel-default">
  <!-- Default panel contents -->
  <div class="panel-heading"><p>Nuevo usuario</p></div>
  <div class="panel-body">
  {{ form_start(form) }}
    {{ form_errors(form) }}
    <div class="row">
        <div class="col-md-4">
            {{ form_row(form.persona) }}
        </div>
        <div class="col-md-2">
            {{ form_row(form.username) }}
        </div>
        <div class="col-md-2">
            {{ form_row(form.password.first) }}
        </div>
        <div class="col-md-2">
            {{ form_row(form.password.second) }}
        </div>
         {#<div class="col-md-2">
        {{ form_row(form.estado) }}
         </div>#}
    </div>
    <div class="row">
        <div class="col-md-4">
            {{ form_row(form.user_roles) }}
        </div>
    </div>
        {#{{ form_row(form.estado) }}#}
        
   
   
             <div class="form-grouphide col-sm-9">
                <a href="{{ path('admin_producto') }}" class="btn btn-default btn-sm" id="cancelarNuevo">Cancelar</a>
                {{ form_widget(form.submit) }}
              </div>
            
    
    {{ form_end(form) }}
  </div>


</div>        

{% endblock %}
    