{% extends 'DGPlusbelleBundle:Layout:general.html.twig' %}

{% block contenido -%}
    
    <div class="panel panel-default">
  <!-- Default panel contents -->
  <div class="panel-heading"<h1>Nueva categoria</h1></div>
  <div class="panel-body">
  {{ form_start(form) }}
    {{ form_errors(form) }}
    <div class="row">
        <div class="col-xs-12 col-sm-12">
        {{ form_row(form.nombre) }}
       {# {{ form_row(form.estado) }}#}
      
        
        </div>
    </div>
    <div class="row">
         <div class="col-xs-12 col-sm-12">
             <div class="modal-footer">
                <a href="{{ path('admin_categoria') }}" class="btn btn-default">Regresar al listado</a>
                {{ form_widget(form.submit) }} 
              </div>
            
        </div>
    </div>
    {{ form_end(form) }}
  </div>


</div>        
    

{% endblock %}
